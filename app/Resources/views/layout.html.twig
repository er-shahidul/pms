{% endjavascripts %}

<script type="text/javascript">
    jQuery(document).ready(function () {
        {% block documentready %}

        App.init();
        FormValidation.init();
        FormComponents.init();
        $('.buyer-group').hide();
        Tasks.initDashboardWidget();
        Index.initPieCharts();

        {% endblock %}
    });
</script>

{% block footer %}
    <div class="footer">
        <div class="footer-inner">
            2013 &copy; Create By RBS.
        </div>
        <div class="footer-tools">
        <span class="go-top">
            <i class="fa fa-angle-up"></i>
        </span>
        </div>
    </div>
{% endblock footer %}

</body>

</html>
